{
  "name": "workoutloud",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "eslint-config-custom": "*",
    "postcss": "^8.4.18",
    "prettier": "latest",
    "tailwindcss": "^3.2.1",
    "turbo": "^1.6.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@react-oauth/google": "^0.4.0",
    "jwt-decode": "^3.1.2",
    "yup": "^0.32.11"
  }
}
